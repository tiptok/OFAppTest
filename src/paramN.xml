<?xml version="1.0" encoding="UTF-8"?>
<Data>
  <param>
    <!--(##需要配置##)服务名-->
    <ServiceName>TopNAS</ServiceName>
    <!--(##需要配置##)服务显示名称-->
    <ServiceDescription>Top7接入服务</ServiceDescription>
	  <!--(##需要配置##)授权码-->
    <AuthCode>g5PFiyT8xPWoJr46PMCgJasHFhO1WrIYIkOVkjSnk5D2DT/FhsIFOWhnvpyJtnwPqT8qUmBn/xMlrSu3n+gidaofTVaXMO2sf/w4FMO+eUkqmeuk2cUmCs/lF0nKJ84Zh2EwrSuiGQSEBlI8F0dJ5jxaUnaWfgdj7V/RpQZZ9E0=</AuthCode>
    <!--(##需要配置##)数据库连接字符串-->
    <ConnectionString>server=192.168.3.87;uid=sa;pwd=top@db123;database=TopDB;Connect Timeout=120;</ConnectionString>
    <!--(##需要配置##)分发监听端口（用于业务数据分发）9996   87 808:9990-->
    <BizPort>9990</BizPort>
    <!--(##需要配置##)终端监听端口（用于接收TOP6通讯服务器/JTB808终端等的数据）9099-->
    <TermPort>9927</TermPort>
    <!--(##需要配置##)接入终端协议类型(部标终端:JTB808;TOP6通讯:TOP6COMM;809接入：JTB809;自有协议:TOP7ZYP;星瑞达:TOP7XRD 单链路位置接入:TOP7POS)-->
    <TermProtocol>JTB809</TermProtocol>
    <!--通讯服务器IP地址(接入终端类型TOP6COMM时必填 用于下发指令)-->
    <CommServerIP></CommServerIP>
    <!--(##需要配置##)日志保存路径-->
    <LogPath>D:\Logs\NasLog</LogPath>
    <!--日志保存天数-->
    <LogSaveDays>3</LogSaveDays>
    <!--日志级别(Error = 0, Warning = 1, Info = 2, Debug = 3)-->
    <LogLevel>3</LogLevel>
    <!--(##需要配置##)缓存文件路径-->
    <CachePath>C:\TopNasCache</CachePath>	
    <!--队列最大缓存报文条数-->
    <MaxCacheCount>10000</MaxCacheCount>  
    <!--终端基本信息缓存刷新时间间隔 单位：秒-->
    <TermCacheSpan>60</TermCacheSpan>
    <!--808设置-->
    <!--808协议注册是否允许重复注册（1允许重复注册，0 不允许）-->
    <EnableRepeatRegister>1</EnableRepeatRegister>
    <!--808协议注册是否验证车牌号（1验证车牌，0不验证）-->
    <CheckPlateNum>0</CheckPlateNum>
    <!--808协议注册是否验证车牌颜色（1验证车牌，0不验证）-->
    <CheckPlateColor>0</CheckPlateColor>
    <!--808协议注册是否验证终端ID（1验证车牌，0不验证）-->
    <CheckTerminalId>0</CheckTerminalId>
    <!--808是否鉴权全通过（1鉴权全通过，0需验证鉴权）-->
    <EnableTermAuthAllPass>0</EnableTermAuthAllPass>

    <!--******************功能设置  1：开启  0：关闭********************************-->
    <!--NAS 川标专版开关  1：开启  0：关闭  默认关闭-->
    <IsOpenSCAnalysis>0</IsOpenSCAnalysis>
    <!--是否开启平台报警分析   1：开启  0：关闭  默认开启-->
    <IsOpenPlatAlarmAnalysis>1</IsOpenPlatAlarmAnalysis>
    <!--是否开启终端报警分析   1：开启  0：关闭 默认开启-->
    <IsOpenTermAlarmAnalysis>1</IsOpenTermAlarmAnalysis>
    <!--是否开启驾驶员分析    1：开启  0：关闭   默认开启-->
    <IsOpenDriverAnalysis>1</IsOpenDriverAnalysis>
    <!--是否开启外设数据分析   1：开启  0：关闭  默认开启-->
    <IsOpenDevDataAnalysis>1</IsOpenDevDataAnalysis>
    <!--是否开启当前位置分析或缓存   1：开启  0：关闭  默认开启-->
    <IsOpenCurPosAnalysis>1</IsOpenCurPosAnalysis>
    <!--是否开启设置车辆限制速度(从车辆配置表里面获取限速值)  1：开启  0：关闭（位置报文）默认开启-->
    <IsOpenSetLimitSpeed>1</IsOpenSetLimitSpeed>
    <!--是否开启偏离路线分析  1：开启  0：关闭 默认不开启-->
    <IsOpenPathAlarmAnalysis>0</IsOpenPathAlarmAnalysis>
    <!--是否开启进出区域分析 1：开启  0：关闭 默认关闭-->
    <IsOpenInOutRegionAnalysis>0</IsOpenInOutRegionAnalysis>
    <!--是否开启进出路线分析 1：开启  0：关闭-->
    <IsOpenInOutPathAnalysis>0</IsOpenInOutPathAnalysis>
    <!--是否开启车辆禁行分析 1：开启  0：关闭（位置报文）默认关闭-->
    <IsOpenCarBanAlarm>0</IsOpenCarBanAlarm>
    <!--是否开启车辆位置标记功能 0:关闭 1:开启 默认关闭-->
    <IsOpenMarkPosition>0</IsOpenMarkPosition>
    <!--是否开启ACC开且未定位分析 0:关闭 1:开启 默认关闭-->
    <IsOpenACCOutLocation>0</IsOpenACCOutLocation>
    <!--是否开启每日第一条油耗数据缓存（网锐）   1：开启  0：关闭-->
    <IsOpenOilDayFirstData>0</IsOpenOilDayFirstData>
    <!--是否开启屏蔽终端报警   1：开启  0：关闭  默认关闭(可上报的报警为需要终端分析报警位 AlarmFlag配置)-->
    <IsOpenShieldTermAlarm>0</IsOpenShieldTermAlarm>
    <!--是否开启屏蔽报警状态位当车辆停车   1：开启  0：关闭  默认关闭-->
    <IsOpenSAFWhenVehicleStop>0</IsOpenSAFWhenVehicleStop>
    <!--屏蔽报警位(车辆停车)-->
    <ShieldAlarmFlag>2305843009222369286</ShieldAlarmFlag>
    <!--是否保存接收到指令的信息到数据库   1：开启  0：关闭  默认开启-->
    <IsSaveRecCmdInfo>1</IsSaveRecCmdInfo>
    <!--CmdCode黑名单（不做保存 使用 ','分割)-->
    <CmdCodeNeverSaveList></CmdCodeNeverSaveList>
    
    <!--数据库设置-->
    <!--解析通讯报文的线程数 默认-1 代表线程数是cpu个数的两倍-->
    <ThreadCountCommEntity>-1</ThreadCountCommEntity>
    <!--正常轨迹数据入库线程数-->
    <ThreadCountNormalPos>-1</ThreadCountNormalPos>
    <!--异常轨迹数据入库线程数-->
    <ThreadCountAbnormalPos>-1</ThreadCountAbnormalPos>
    <!--当前位置数据入库线程数-->
    <ThreadCountCurrPos>-1</ThreadCountCurrPos>
    <!--IO外设数据入库线程数-->
    <ThreadCountIODevice>-1</ThreadCountIODevice>
    <!--命令应答入库线程数-->
    <ThreadCountOrderResult>-1</ThreadCountOrderResult>
    <!--其他数据入库线程数-->
    <ThreadCountOtherBiz>-1</ThreadCountOtherBiz> 
    <!--报警分析线程数-->
    <ThreadCountAlarmAnalysis>-1</ThreadCountAlarmAnalysis>
    <!--位置一次性入库数目 默认一次 1000条-->
    <CurPosOnceDBTaskNum>1000</CurPosOnceDBTaskNum>
    
    <!--****************************报警设置****************************-->
    <!--需要分析的 报警类型-->
    <AlarmFlag>2199023255551</AlarmFlag>
    <!--需要分析的 车辆状态-->
    <VehicleState>769</VehicleState>
    <!--停车持续时长(达到停车时长的才做停车分析,默认两分钟) 单位:分钟-->
    <VehicleStopDuration>2</VehicleStopDuration>
    <!--路宽 (平台偏离路线配置 单位:米)-->
    <RoadWidth>500</RoadWidth>
    <!--平台超速预警差值 单位 km/h(默认 5km/h)-->
    <WarnSpeedSpan>5</WarnSpeedSpan>
    
    

    <!--****************************短信服务设置*********************************-->
    <!--是否开启报警短信发送服务 0：关闭 1：开启(有开启发送短信 需要配置为1)-->
    <IsOpenAlarmSmsService>0</IsOpenAlarmSmsService>
    <!--短信网关服务器IP-->
    <SmsServer>218.5.10.82</SmsServer>
    <!--短信网关端口-->
    <SendSmsPort>9032</SendSmsPort>
    <!--短信发送者-->
    <SmsSender>福州星冠智通</SmsSender>
    <!--位置解析服务地址-->
    <GisUrl>http://127.0.0.1:8088/map/OpenLayers/MapService.asmx/QueryAllLayerByPoint?x={x}&amp;y={y}</GisUrl>
    <!--语音消息格式 速度:{0}-->
    <SendTextMsgFormat>车已超速,请减速慢行!当前速度{0}千米/小时</SendTextMsgFormat>
    <!--版本标识 1：正常版本 2：网锐油耗版本 4:天凯风林 油耗扩展-->
    <VersionType>1</VersionType>
    <!--是否为副接入服务器：0 主接入，1 副接入服务器（不回复应答，用于平台并行），默认值：0-->
    <IsSecondSys>0</IsSecondSys>
    
    <!--*********************Top6接入专用*************************************-->
    <!--是否使用营运停运状态当出租车空重车状态  1：开启  0：关闭-->
    <IsOperateToTransportStatus>0</IsOperateToTransportStatus>

    <!--*********************自有协议接入（专用）******************************-->
    <!--是否开启平台查岗功能   1：开启  0：关闭（自有协议专用）默认关闭,配置自有协议接入需要开启-->
    <IsOpenPlatFormQuery>0</IsOpenPlatFormQuery>
    <!--设置 平台自动查岗时间间隔  单位:秒 默认30秒-->
    <PlatFormQuerySpan>30</PlatFormQuerySpan>

    <!--************************809接入配置(809接入专用)************************************-->
    <!--地区前缀 区分不同地区的差异 默认不配置-->
    <AreaPrefix></AreaPrefix>
    <!--加密密钥-->
    <Key>0</Key>
    <!--是否 1：加密 0：不加密-->
    <IsEncrypt>0</IsEncrypt>
    <!--数据加密参数M1-->
    <M1>10000000</M1>
    <!--数据加密参数IA1-->
    <IA1>20000000</IA1>
    <!--数据加密参数IC1-->
    <IC1>30000000</IC1>
    <!--接入码-->
    <AccessCode>12345678</AccessCode>
    <!--从链路登录校验码-->
    <VerifyCode>789</VerifyCode>
    <!--协议版本号-->
    <ProtocolVesion>1.0.1</ProtocolVesion>
    <!--从链路连接IP是否直接使用主链路连接过来的IP， False-默认使用登录信息中的从链路IP， True-使用主链路IP-->
    <IsSubLinkUseMainLinkIP>False</IsSubLinkUseMainLinkIP>
    
    <!--************************位置接入专用（TopPos）************************************-->
    <!--位置接入协议名称 船舶:Sm1601  部标809:JTB808-->
    <PosParseTypeName>Sm1601</PosParseTypeName>
</param>
</Data>